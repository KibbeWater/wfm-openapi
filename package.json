{
  "name": "wfm-openapi",
  "version": "0.21.2",
  "description": "OpenAPI and AsyncAPI documentation for warframe.market API",
  "type": "module",
  "scripts": {
    "build": "npm run build:openapi && npm run build:asyncapi && npm run build:static",
    "build:openapi": "tsp compile src/main.tsp",
    "build:asyncapi": "cp asyncapi/websockets.yaml dist/asyncapi.yaml",
    "build:static": "cp static/index.html dist/index.html",
    "serve": "npx serve dist",
    "clean": "rm -rf dist",
    "watch": "tsp compile src/main.tsp --watch"
  },
  "keywords": [
    "warframe",
    "warframe-market",
    "openapi",
    "asyncapi",
    "api-documentation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typespec/compiler": "^1.7.1",
    "@typespec/http": "^1.7.0",
    "@typespec/openapi": "^1.7.0",
    "@typespec/openapi3": "^1.7.0",
    "serve": "^14.2.5"
  }
}
